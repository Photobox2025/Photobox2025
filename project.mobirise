          "_name": "form01",
          "_sourceTheme": "ridem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"form01 ridem5\" group=\"Form\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked=\"\">\n        <input type=\"color\" title=\"Contacts Active Color\" value=\"#ffffff\" name=\"activeCont\" condition=\"showContacts\" selected=\"\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked=\"\">\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" condition=\"showSocial\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Social Amount\" name=\"soc\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" value=\"#000000\" name=\"socColor\" condition=\"showSocial\" selected=\"\">\n        <input type=\"color\" title=\"Active Color\" value=\"#ffffff\" name=\"active\" condition=\"showSocial\" selected=\"\">\n        <header>Form</header>\n        <input type=\"color\" title=\"Input BG\" value=\"#ffffff\" name=\"inputBg\" selected=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#CECDC7\" selected=\"\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12\" mbr-if=\"showTitle || showText\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">\n                        <b>Enjoy the Ride</b>\n                    </h2>\n                    <div class=\"text-wrapper\">\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .text-wrapper\">\n                            Are you ready to zip through the city streets with ease and style? Say hello to the future\n                            of\n                            urban transportation with our electric scooter rental service! Whether you're a local\n                            looking to\n                            breeze through your daily commute.\n                        </p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div mbr-form=\"\" class=\"mbr-form form-wrapper\">\n<!--Formbuilder Form-->\n<form action=\"matthieu.nicaise@outlook.com\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n<div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\"> Oops...! some problem! </div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3 mb-3\" data-for=\"\">\n<label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"\" class=\"form-control-label mbr-fonts-style\">True Name</label>\n<input type=\"text\" name=\"\" placeholder=\"\" data-form-field=\"\" mbr-theme-style=\"display-4\" class=\"form-control\" value=\"\">\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3\" data-for=\"email\">\n<label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"email\" class=\"form-control-label mbr-fonts-style\">E-MAIL*</label>\n<input type=\"email\" name=\"email\" data-form-field=\"email\" mbr-theme-style=\"display-4\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"number\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"number\" class=\"form-control-label mbr-fonts-style\">PHONE*</label>\n<input type=\"number\" name=\"number\" data-form-field=\"number\" mbr-theme-style=\"display-4\" class=\"form-control\" max=\"100\" min=\"0\" step=\"1\" value=\"\">\n</div>\n<div data-for=\"tel\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"tel\" class=\"form-control-label mbr-fonts-style\">Phone</label>\n<input type=\"tel\" name=\"tel\" data-form-field=\"tel\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"+32 456 12 34 56\">\n</div>\n<div data-for=\"text\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"text\" class=\"form-control-label mbr-fonts-style\">COMPANY</label>\n<input type=\"text\" name=\"text\" data-form-field=\"text\" mbr-theme-style=\"display-4\" class=\"form-control\" value=\"\">\n</div>\n<div data-for=\"textarea\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text=\"\" mbr-theme-style=\"display-4\" for=\"textarea\" class=\"form-control-label mbr-fonts-style\">MESSAGE</label>\n<textarea name=\"textarea\" data-form-field=\"textarea\" mbr-theme-style=\"display-4\" class=\"form-control\"></textarea>\n</div>\n<div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col mbr-section-btn\">\n<a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\" class=\"w-100 w-100 w-100 btn btn-primary\">SEND NOW</a>\n</div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n            </div>\n            <div class=\"col-12 col-lg-5\">\n                <div class=\"content-wrapper\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\">\n                        <b>Contact for Booking</b>\n                    </h3>\n                    <div class=\"list-wrapper\" mbr-if=\"showContacts\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap\" data-multiline=\"\" mbr-article=\"\">\n                            <li class=\"item-wrap\"><b>32 Rue de Rivoli Paris, France</b></li>\n                            <li class=\"item-wrap\"><b>mobi@rise.com</b></li>\n                            <li class=\"item-wrap\">+985(0)65 342 110</li>\n                            <li class=\"item-wrap\"><b>ride@m5.com</b></li>\n                            <li class=\"item-wrap\">+985(0)63 123 110</li>\n                            <li class=\"item-wrap\">Inst: Ruth Rhodes</li>\n                        </ul>\n                    </div>\n                    <div class=\"content-wrap\" mbr-if=\"showSocial\">\n                        <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showDesc\" data-app-selector=\".mbr-desc, .social-wrapper\">\n                            <b>Connect with us on social media</b>\n                        </p>\n                        <div class=\"social-wrapper\">\n                            <div class=\"social-wrap\">\n                                <div class=\"soc-item\">\n                                    <a href=\"https://mobiri.se/\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon socicon-facebook\"></span>\n                                    </a>\n                                </div>\n                                <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                                    <a href=\"https://mobiri.se/\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                                    </a>\n                                </div>\n                                <div class=\"soc-item\" mbr-if=\"soc > 2\">\n                                    <a href=\"https://mobiri.se/\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon socicon-tiktok\"></span>\n                                    </a>\n                                </div>\n                                <div class=\"soc-item\" mbr-if=\"soc > 3\">\n                                    <a href=\"https://mobiri.se/\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont socicon socicon-linkedin\"></span>\n                                    </a>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
